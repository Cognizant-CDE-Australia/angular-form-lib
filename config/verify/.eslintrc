# START_CONFIT_GENERATED_CONTENT
extends: defaults/configurations/google

env:
  jasmine: true
  node: true
  browser: true
  commonjs: true    # For Webpack, CommonJS
  protractor: true
  es6: true

confit-globals: &confit-globals
  angular: true

ecmaFeatures:
  modules: true

# END_CONFIT_GENERATED_CONTENT
globals:
  <<: *confit-globals

rules:
  max-len:
    - 2       # Warning
    ####- 200     # Line Length
    - 500

  dot-location:
    - 1       #error
    - property

  no-unused-vars:
    - 1
