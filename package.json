{
  "name": "angular-form-lib",
  "version": "0.0.0-semantically-released",
  "description": "AngularJS 1.2+ directives for controlling form behaviour using policies with accessible form controls",
  "keywords": [
    "IE8",
    "angular",
    "form"
  ],
  "homepage": "http://odecee.github.io/angular-form-lib",
  "bugs": {
    "url": "https://github.com/odecee/angular-form-lib/issues"
  },
  "license": "Apache-2.0",
  "files": [
    "dist/es5/",
    "src/modules",
    "package.json",
    "CONTRIBUTING.md",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/es5/ngFormLib.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/odecee/angular-form-lib.git"
  },
  "scripts": {
    "build": "npm-run-all clean:prod build:prod build:docs",
    "build:dev": "webpack-dev-server --progress --config config/webpack/dev.webpack.config.js --hot",
    "build:docs": "webpack --progress --config config/webpack/docs.webpack.config.js",
    "build:prod": "webpack --progress --config config/webpack/prod.webpack.config.js",
    "build:serve": "npm-run-all build serve:prod",
    "clean:dev": "rimraf dev/",
    "clean:prod": "rimraf dist/",
    "dev": "npm-run-all clean:dev --parallel verify:watch build:dev serve:dev",
    "publish:docs": "grunt publishDocs",
    "pre-release": "npm-run-all verify test:coverage build",
    "semantic-release": "semantic-release pre && npm run publish:docs && npm publish && semantic-release post",
    "serve:dev": "echo Starting DEV server",
    "serve:prod": "npm run serve:prod:https",
    "serve:prod:http": "serve -C -p 4000 dist/",
    "serve:prod:https": "serve-https -p 4000 -d dist/ --servername localhost",
    "start": "npm run dev",
    "test": "npm run test:unit",
    "test:coverage": "npm run test:unit:once",
    "test:unit": "karma start ./config/testUnit/karma.conf.js",
    "test:unit:debug": "karma start ./config/testUnit/karma.conf.js --debug",
    "test:unit:once": "karma start --singleRun=true ./config/testUnit/karma.conf.js",
    "verify": "npm-run-all verify:css verify:js --silent",
    "verify:css": "stylint -c config/verify/.stylintrc src/ && echo ✅ verify:css success",
    "verify:css:watch": "chokidar 'src/**/*.styl' -c 'npm run verify:css' --initial --silent",
    "verify:js": "eslint -c config/verify/.eslintrc \"src/**/*.js\" \"unitTest/**/*.js\" \"config/**/*.js\" && echo ✅ verify:js success",
    "verify:js:watch": "chokidar 'src/**/*.js' 'unitTest/**/*.js' 'config/**/*.js' -c 'npm run verify:js' --initial --silent",
    "verify:watch": "npm-run-all --parallel verify:css:watch verify:js:watch --silent"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/release/commitMessageConfig.js"
    },
    "ghooks": {
      "commit-msg": "./node_modules/cz-customizable-ghooks/lib/index.js $2",
      "pre-push": "npm-run-all verify test:unit:once --silent"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "angular": "1.2.29",
    "angular-animate": "1.2.29",
    "angular-mocks": "1.2.29",
    "angular-motion": "0.3.4",
    "angular-scroll": "0.7.1",
    "angular-strap": "2.2.0",
    "angular-translate": "2.7.2",
    "autoprefixer": "6.3.6",
    "babel-core": "6.4.5",
    "babel-loader": "6.2.1",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "babel-runtime": "6.3.19",
    "bootstrap-styl": "3.3.1",
    "chokidar-cli": "1.2.0",
    "css-loader": "0.23.1",
    "cz-customizable": "4.0.0",
    "cz-customizable-ghooks": "1.2.0",
    "eslint": "1.10.3",
    "eslint-config-defaults": "8.0.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "ghooks": "1.2.1",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-extend-config": "0.9.3",
    "grunt-gh-pages": "1.1.0",
    "highlightjs": "8.7.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.7.1",
    "isparta-instrumenter-loader": "1.0.0",
    "jasmine-core": "2.4.1",
    "jasmine-reporters": "2.1.1",
    "jasmine-spec-reporter": "2.4.0",
    "json-loader": "0.5.4",
    "karma": "0.13.22",
    "karma-chrome-launcher": "0.2.3",
    "karma-coverage": "0.5.5",
    "karma-jasmine": "0.3.8",
    "karma-junit-reporter": "0.4.2",
    "karma-ng-html2js-preprocessor": "0.2.0",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.23",
    "karma-threshold-reporter": "0.1.15",
    "karma-webpack": "1.7.0",
    "load-grunt-tasks": "3.4.0",
    "lodash": "3.10.1",
    "npm-run-all": "2.1.1",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.7",
    "postcss-loader": "0.8.2",
    "rimraf": "2.5.2",
    "semantic-release": "4.3.5",
    "serve": "1.4.0",
    "serve-https": "1.5.3",
    "style-loader": "0.13.0",
    "stylint": "1.5.6",
    "stylus": "0.54.5",
    "stylus-loader": "2.1.1",
    "webpack": "1.12.11",
    "webpack-dev-server": "1.14.1"
  }
}
